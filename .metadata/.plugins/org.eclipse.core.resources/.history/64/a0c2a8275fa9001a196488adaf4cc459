package com.method;

interface DM
{
	public void abstractmethod();
	default void show()
	{
		System.out.println("default method works in interface");
	}
}
class A implements DM
{
	public void abstractmethod()
	{
		System.out.println("only one abstract method allowed");
	}
//	@Override
//	public  void show()
//	{
//		System.out.println("override default method allowed");
//	}
}

public class DefaultMethod
{
	public static void main(String... d)
	{
		
		DM obj=new A();//cannot create object of interface but can create reference of interfece
		obj.abstractmethod();
		
		obj.show();
	}

}
